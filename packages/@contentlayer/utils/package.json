{
  "name": "@contentlayer/utils",
  "version": "0.0.23-dev.8",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./effect": {
      "import": "./dist/esm/effect/index.js",
      "require": "./dist/cjs/effect/index.js"
    },
    "./effect/Tracing/Enable": {
      "import": "./dist/esm/effect/Tracing/Enable.js",
      "require": "./dist/cjs/effect/Tracing/Enable.js"
    },
    "./node": {
      "import": "./dist/esm/node/index.js",
      "require": "./dist/cjs/node/index.js"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "effect": [
        "./dist/esm/effect"
      ],
      "effect/Tracing/Enable": [
        "./dist/esm/effect/Tracing/Enable"
      ],
      "node": [
        "./dist/esm/node"
      ]
    }
  },
  "scripts": {
    "_prepack": "yarn build",
    "dev": "rm -rf dist && tsc --watch",
    "build": "(run g:build:lib & run g:build:types) && echo Build complete"
  },
  "dependencies": {
    "@effect-ts/core": "^0.43.9",
    "@effect-ts/otel": "^0.5.2",
    "@effect-ts/otel-node": "^0.5.2",
    "@opentelemetry/api": "^1.0.1",
    "@opentelemetry/exporter-collector": "^0.24.0",
    "@opentelemetry/node": "^0.22.0",
    "@opentelemetry/resources": "^0.24.0",
    "@opentelemetry/semantic-conventions": "^0.24.0",
    "@opentelemetry/tracing": "^0.22.0",
    "chokidar": "^3.5.2",
    "hash-wasm": "^4.9.0",
    "inflection": "^1.13.1",
    "oo-ascii-tree": "^1.34.0",
    "ts-pattern": "^3.2.1",
    "type-fest": "^1.2.1"
  },
  "devDependencies": {
    "@types/inflection": "^1"
  }
}
