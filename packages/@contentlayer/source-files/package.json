{
  "name": "@contentlayer/source-files",
  "version": "0.0.20",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./schema": {
      "import": "./dist/esm/schema/index.js",
      "require": "./dist/cjs/schema/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/types/schema"
      ]
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "dev": "rm -rf dist && tsc --watch",
    "build": "(yarn workspaces foreach -vpR -j=6 run g:build:lib & run g:build:types) && echo Build complete"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "chokidar": "^3.5.2",
    "glob": "^7.1.7",
    "glob-promise": "^4.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "minimatch": "^3.0.4",
    "rxjs": "^7.1.0",
    "ts-pattern": "^3.2.1",
    "unified": "^9.2.1"
  }
}
