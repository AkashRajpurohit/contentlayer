{
  "name": "@contentlayer/source-files",
  "version": "0.0.24-dev.9",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./schema/defs": {
      "import": "./dist/esm/schema/defs/index.js",
      "require": "./dist/cjs/schema/defs/index.js"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/esm/schema"
      ]
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {},
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "chokidar": "^3.5.2",
    "glob": "^7.1.7",
    "glob-promise": "^4.1.0",
    "gray-matter": "^4.0.3",
    "minimatch": "^3.0.4",
    "ts-pattern": "^3.3.1",
    "unified": "^10.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.14.1",
    "@scripts/jest": "workspace:*",
    "@types/faker": "^5.5.8",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16.9.2",
    "@types/yaml": "^1",
    "faker": "^5.5.3"
  }
}
