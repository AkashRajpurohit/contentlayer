{
  "name": "@contentlayer/source-files",
  "version": "0.0.23-dev.8",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./schema/defs": {
      "import": "./dist/esm/schema/defs/index.js",
      "require": "./dist/cjs/schema/defs/index.js"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/esm/schema"
      ]
    }
  },
  "scripts": {
    "_prepack": "yarn build",
    "dev": "rm -rf dist && tsc --watch",
    "build": "run g:build && echo Build complete"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "chokidar": "^3.5.2",
    "glob": "^7.1.7",
    "glob-promise": "^4.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "minimatch": "^3.0.4",
    "ts-pattern": "^3.2.1",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@scripts/jest": "workspace:*",
    "@types/faker": "^5",
    "@types/js-yaml": "^4.0.1",
    "@types/minimatch": "^3.0.4",
    "@types/node": "^15.12.0",
    "faker": "^5.5.3"
  }
}
