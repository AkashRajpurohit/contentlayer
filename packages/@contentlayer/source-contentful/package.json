{
  "name": "@contentlayer/source-contentful",
  "version": "0.0.2-12",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prepack": "yarn build",
    "dev": "rm -rf dist *.tsbuildinfo && tsc --watch",
    "build": "rm -rf dist && yarn build:esm && yarn build:cjs && echo Build complete",
    "build:esm": "tsc --build tsconfig.json",
    "build:cjs": "tsc --build tsconfig.cjs.json"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "contentful-management": "7.14.0",
    "rxjs": "^7.0.0"
  }
}
