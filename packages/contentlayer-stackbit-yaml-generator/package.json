{
  "name": "contentlayer-stackbit-yaml-generator",
  "version": "0.0.21",
  "bin": "./dist/cli/index.js",
  "main": "./dist/cjs/lib/index.js",
  "module": "./dist/esm/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "scripts": {
    "prepack": "yarn build",
    "build": "(yarn workspaces foreach -vpR -j=6 run g:build:lib & run g:build:types) && yarn build:cli && echo Build complete",
    "build:cli": "rm -rf dist/cli && esbuild --bundle src/cli/index.ts --outfile=dist/cli/index.js --platform=node --sourcemap --external:esbuild && chmod +x ./dist/index.js"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "@stackbit/sdk": "^0.1.15",
    "clipanion": "^3.0.0-rc.12",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "contentlayer": "workspace:*",
    "esbuild": "^0.12.14",
    "typescript": "^4.3.5"
  }
}
