{
  "name": "contentlayer-stackbit-yaml-generator",
  "version": "0.0.11",
  "bin": "./dist/index.js",
  "scripts": {
    "prepack": "yarn build",
    "build": "yarn build:cli",
    "build:cli": "rm -rf dist && esbuild --bundle src/index.ts --outfile=dist/index.js --platform=node --sourcemap --external:esbuild && chmod +x ./dist/index.js"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "@stackbit/sdk": "^0.1.15",
    "clipanion": "^3.0.0-rc.12",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "contentlayer": "workspace:*",
    "esbuild": "^0.12.14",
    "typescript": "^4.3.5"
  }
}
