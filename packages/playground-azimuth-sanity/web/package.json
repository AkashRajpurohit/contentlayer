{
  "name": "playground-azimuth-sanity-web",
  "scripts": {
    "dev": "next",
    "build": "yarn generate && yarn fetch && next build",
    "generate": "ESBUILD_BINARY_PATH=$(which esbuild) sourcebit generate --schema ./src/schema/index.ts",
    "fetch": "ESBUILD_BINARY_PATH=$(which esbuild) sourcebit fetch --schema ./src/schema/index.ts --content 'content/**/*.{md,json,yaml}' --cache ./src/sourcebit.json"
  },
  "dependencies": {
    "@sourcebit/cli": "workspace:*",
    "@sourcebit/sdk": "workspace:*",
    "date-fns": "^2.18.0",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "typescript": "^4.1.5"
  }
}
