{
  "name": "playground-azimuth-sanity-web",
  "scripts": {
    "dev": "next",
    "build": "yarn generate && yarn fetch && next build",
    "generate": "ESBUILD_BINARY_PATH=$(which esbuild) sourcebit generate --config ./sourcebit/sourcebit.ts",
    "fetch": "ESBUILD_BINARY_PATH=$(which esbuild) sourcebit fetch --config ./sourcebit/sourcebit.ts"
  },
  "dependencies": {
    "@sourcebit/cli": "workspace:*",
    "@sourcebit/client": "workspace:*",
    "@sourcebit/core": "workspace:*",
    "@sourcebit/source-sanity": "workspace:*",
    "@sourcebit/types": "workspace:*",
    "date-fns": "^2.18.0",
    "marked": "^2.0.1",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-html-parser": "^2.0.2",
    "react-script-tag": "^1.1.2",
    "sass": "^1.32.7"
  },
  "devDependencies": {
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-helmet": "^6",
    "typescript": "^4.1.5"
  }
}
