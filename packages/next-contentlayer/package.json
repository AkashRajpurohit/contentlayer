{
  "name": "next-contentlayer",
  "version": "0.0.6",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./hooks": {
      "import": "./dist/esm/hooks.js",
      "require": "./dist/cjs/hooks.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index"
      ],
      "hooks": [
        "./dist/hooks"
      ]
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "(yarn workspaces foreach -vpR -j=6 run g:build:lib & run g:build:types) && echo Build complete"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "rxjs": "^7.1.0"
  },
  "peerDependencies": {
    "next": "^10"
  },
  "devDependencies": {
    "next": "^11.0.1",
    "typescript": "^4.2.4"
  }
}
