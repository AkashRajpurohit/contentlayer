{
  "name": "next-contentlayer",
  "version": "0.0.28-dev.17",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./hooks": {
      "import": "./dist/esm/hooks/index.js",
      "require": "./dist/cjs/hooks/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index"
      ],
      "hooks": [
        "./dist/esm/hooks/index"
      ]
    }
  },
  "scripts": {
    "prepack": "sed '/^import /d' dist/cjs/index.js > dist/cjs/index.cjs"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "rxjs": "^7.1.0"
  },
  "peerDependencies": {
    "next": "^11 || ^12",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "next": "^12.0.2",
    "typescript": "^4.4.4"
  }
}
