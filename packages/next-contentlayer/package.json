{
  "name": "next-contentlayer",
  "version": "0.0.23-dev.8",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./hooks": {
      "import": "./dist/esm/hooks.js",
      "require": "./dist/cjs/hooks.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index"
      ],
      "hooks": [
        "./dist/esm/hooks"
      ]
    }
  },
  "scripts": {
    "_prepack": "yarn build",
    "build": "run g:build && echo Build complete"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "rxjs": "^7.1.0"
  },
  "peerDependencies": {
    "next": "^11"
  },
  "devDependencies": {
    "next": "^11.1.2",
    "typescript": "^4.4.3"
  }
}
